<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.0.0" immediate="true" name="org.apache.sling.servlets.get.impl.RedirectServlet">
    <implementation class="org.apache.sling.servlets.get.impl.RedirectServlet"/>
    <service servicefactory="false">
        <provide interface="javax.servlet.Servlet"/>
    </service>
    <property name="service.description" value="Request Redirect Servlet"/>
    <property name="service.vendor" value="The Apache Software Foundation"/>
    <property name="sling.servlet.resourceTypes" value="sling:redirect"/>
